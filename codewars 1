array = [1, 2, 1, 5] # массив лягушенка фрогии
def count_jumps(arr):
    pos = 0
    jumps = 0
    visited = set()
    
    while 0 <= pos < len(arr):
        if pos in visited:
            return -1
        visited.add(pos)
        pos += arr[pos]
        jumps += 1
    
    return jumps

print(count_jumps(array))
